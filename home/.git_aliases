# git things!
alias g='git'
alias murder='git branch --merged | grep -v "\*" | grep -v  "master\|staging" | xargs -n 1 git branch -d'
alias gc='git commit -v'
alias gca='git commit -av'
alias ga='git add .'
alias gd='git diff'
alias gds='git diff --staged'
alias gco='git checkout'
alias gcom='git checkout staging'
alias gcob='git checkout -b'
alias gb='git branch'
alias gs='git status'
alias gl='git log --oneline --pretty'
alias gp='git pull'
alias gph='git push --set-upstream origin `git rev-parse --abbrev-ref HEAD`'
alias reset='git reset HEAD --hard'
